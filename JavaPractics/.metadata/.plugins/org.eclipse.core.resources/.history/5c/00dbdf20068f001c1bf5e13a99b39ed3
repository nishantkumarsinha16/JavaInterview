package org.java.programs;

public class ReserveseString {
	String s = "Selenium";

	public static void main(String[] args) {
		// String is inmuttable
		

	}

	public static void approchOne() {
		
		String rev = "";
		int count = s.length();

		for (int i = count - 1; i >= 0; i--) {
			rev = rev + s.charAt(i);
		}
		System.out.println(rev);

	}

}
